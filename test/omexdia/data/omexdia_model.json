{

"auxx" : {
  "solid2liquid" : "(1 - POR)/POR",
  "lim_minOxic" : "O2/(O2 + hsatO2_minOxic)",
  "lim_minAnox" : "(1 - O2/(O2 + hsatO2_minAnox )) * (1 - NO3/(NO3 + hsatNO3_minAnox))",
  "lim_minDeni" : "(1 - O2/(O2 + hsatO2_minDeni)) * NO3/(NO3 + hsatNO3_minDeni)",
  "rescale" : "1/(lim_minOxic + lim_minAnox + lim_minDeni)"
},

"proc" : {
  "minOxicF" : "rminF * OCF * lim_minOxic * rescale * solid2liquid",
  "minOxicS" : "rminS * OCS * lim_minOxic * rescale * solid2liquid",
  "minAnoxF" : "rminF * OCF * lim_minAnox * rescale * solid2liquid",
  "minAnoxS" : "rminS * OCS * lim_minAnox * rescale * solid2liquid",
  "minDeniF" : "rminF * OCF * lim_minDeni * rescale * solid2liquid",
  "minDeniS" : "rminS * OCS * lim_minDeni * rescale * solid2liquid",
  "nitrif" :  "rnitrif * NH3 * O2/(O2 + hsatO2_nitrif)",
  "odu_reox" : "rODUox * ODU * O2/(O2 + hsatO2_ODUox)"
},

"stox" : {
  "OCF" : {
    "minOxicF" : "-1",
    "minAnoxF" : "-1",
    "minDeniF" : "-1"
  },
  "OCS" : {
    "minOxicS" : "-1",
    "minAnoxS" : "-1",
    "minDeniS" : "-1"
  },
  "O2" : {
    "minOxicF" : "-stoi_O2_C",
    "minOxicS" : "-stoi_O2_C",
    "nitrif"   : "-stoi_O2_NH3",
    "odu_reox" : "-stoi_O2_ODU"
  },
  "NO3": {
    "minDeniF" : "-stoi_NO3_C",
    "minDeniS" : "-stoi_NO3_C",
    "nitrif"   : "stoi_NO3_NH3"
  },
  "NH3": {
    "minOxicF" : "stoi_N_C_OCF / (1+adsNH3)",
    "minOxicS" : "stoi_N_C_OCS / (1+adsNH3)",
    "minAnoxF" : "stoi_N_C_OCF / (1+adsNH3)",
    "minAnoxS" : "stoi_N_C_OCS / (1+adsNH3)",
    "minDeniF" : "stoi_N_C_OCF / (1+adsNH3)",
    "minDeniS" : "stoi_N_C_OCS / (1+adsNH3)",
    "nitrif" :  "-1 / (1+adsNH3)"
  },
  "ODU" : {
    "minAnoxF": "(1 - pODUdepo) * stoi_ODU_C",
    "minAnoxS": "(1 - pODUdepo) * stoi_ODU_C",
    "odu_reox": "-1"
  },
  "POR" : {
    "minOxicF" : 0
  }
}

}

